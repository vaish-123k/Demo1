pipeline {
    agent any

 

    stages {
        stage('One') {
            steps {

 

                echo 'Hi,welcome to Unext...'

 

        }
}
        stage('Two') {
            steps {

 

                input('Do you want to proceed?')

}
        }
        stage('Three') {
           when {
             not {
               branch "master"
             }
           }
steps           {
            echo 'hello'
          }
        }
      stage('Four'){
        parallel {
          stage('Unit Test'){
            steps{
              echo 'Running the unit testcases....'
            }
          }
          stage('Integration Test'){

          steps{
              echo 'Running the Integration  testcases....'
            }   

    }
}
}
    }
}
